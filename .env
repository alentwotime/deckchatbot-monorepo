AI_PROVIDER=ollama

# Ollama Configuration
# The name of the model to use with Ollama.
OLLAMA_MODEL_NAME=llava-deckbot

# OpenAI Configuration
# Your OpenAI API key. Required if AI_PROVIDER is set to 'openai'.
OPENAI_API_KEY=
